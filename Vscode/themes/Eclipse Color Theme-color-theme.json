{
  "name": "Eclipse Color Theme",
  "type": "light",
  "colors": {
    "editor.background": "#f5f6f9",
    "editor.foreground": "#000000"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#acacac" // #5F8FBF
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#1100ff" // #bf00b8 #1100ff #05b361 "#00bd5e"
      }
    },
    {
      "name": "String: Escape sequences and placeholders",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#7090FF"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "keyword.other.unit"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffa600" // #169f58
      }
    },
    {
      "name": "Keywords and built-in types",
      "scope": [
        "keyword",
        "storage.type.primitive",
        "storage.type.class",
        "storage.type.struct",
        "storage.type.enum",
        "storage.type.modifier",
        "storage.type.namespace",
        "storage.type.template",
        "storage.type.function",
        "storage.type.union",
        "storage.type.js",
        "storage.type.interface",
        "storage.type.impl",
        "storage.type.trait",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.word",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.logical.python",
        "constant.language",
        "variable.language",
        "keyword.declaration.function",
        "keyword.declaration.class",
        "keyword.declaration.struct",
        "keyword.declaration.enum",
        "keyword.declaration.union",
        "keyword.declaration.trait",
        "keyword.declaration.interface",
        "keyword.declaration.impl",
        "keyword.declaration.type"
      ],
      "settings": {
        "fontStyle": "", // bold
        "foreground": "#db057c" //#800555 #f92672 #d0044f #fd971f #8c0f5c #651fff #D2054E #db057c
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#000000" //#000000 #fd971f
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#db057c" //#00000 #f92672
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "storage.type"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#800555" //#800555
      }
    },
    {
      "name": "Template Types",
      "scope": [
        "entity.name.type.template"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#336633"
      }
    },
    {
      "name": "CUDA Kernel Brackets",
      "scope": [
        "meta.kernel-call punctuation.section.kernel"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#336633"
      }
    },
    {
      "name": "CUDA Kernel Call",
      "scope": [
        "meta.kernel-call entity.name.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#336633"
      }
    },
    {
      "name": "Object Members and Attributes",
      "scope": [
        "variable.other.member",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#333333",
        "fontStyle": ""
      }
    },
    {
      "name": "Functions", // Functions
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "fontStyle": "bold", //bold
        "foreground": "#000000" //#000000 #fd971f #169f58 #31a200 #800555 #db057c #651fff #5f02f3
      }
    },
    {
      "name": "Preprocessor: Other",
      "scope": [
        "meta.preprocessor"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#808080"
      }
    },
    {
      "name": "Preprocessor: Directives",
      "scope": [
        "meta.preprocessor keyword.control.directive"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#7F3F6A" //#7F3F6A #f92672
      }
    },
    {
      "name": "Preprocessor: Headers",
      "scope": [
        "meta.preprocessor string.quoted"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9580FF"
      }
    },
    {
      "name": "SDK types and functions", // este es el require y esas cosa
      "scope": [
        "support.function",
        "support.type.python",
        "support.type.posix-reserved"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#db057c" // #642880 #db057c
      }
    },
    {
      "name": "Python: self",
      "scope": [
        "variable.parameter.function.language.special.self",
        "variable.language.special.self",
        "variable.parameter.function.language.special.cls",
        "variable.language.special.cls"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#7D7D7D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#406080",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#800555",
        "fontStyle": "" //bold
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#800555",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML/XML Meta information",
      "scope": [
        "meta.tag.preprocessor entity.name.tag",
        "meta.tag.preprocessor entity.other.attribute-name",
        "meta.tag.preprocessor string",
        "meta.tag.preprocessor text",
        "meta.tag.metadata.doctype entity.name.tag",
        "meta.tag.metadata.doctype entity.other.attribute-name",
        "meta.tag.metadata.doctype string",
        "meta.tag.metadata.doctype text"
      ],
      "settings": {
        "foreground": "#5F8FBF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Section Heading",
      "scope": [
        "entity.name.section",
        "punctuation.definition.heading"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#800555"
      }
    },
    {
      "name": "Markup: Raw Inline",
      "scope": [
        "markup.inline.raw",
        "punctuation.definition.raw"
      ],
      "settings": {
        "foreground": "#FF00AA",
        "fontStyle": ""
      }
    },
    {
      "name": "Markup: Lists",
      "scope": [
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "TODOs",
      "scope": [
        "keyword.codetag"
      ],
      "settings": {
        "foreground": "#86A3BF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support"
      ],
      "settings": {
        "foreground": "#000000", // #336633 #169f58
        "fontStyle": ""
      }
    }
  ]
}
